{
  "name": "changelog-releasenotes-action",
  "version": "2.0.0",
  "description": "Generate release notes from CHANGELOG.md and upload them into github release description",
  "main": "dist/index.js",
  "scripts": {
    "build": "ncc build src/index.js -o dist",
    "lint": "eslint .",
    "test": "jest --passWithNoTests",
    "temptest": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "GitHub",
    "Actions",
    "JavaScript",
    "Changelog",
    "Release",
    "Notes"
  ],
  "author": "kuvaus",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kuvaus/changelog-releasenotes-action.git"
  },
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "@octokit/rest": "^20.0.1",
    "node-fetch": "^3.3.1"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.45.0",
    "jest": "^29.6.1"
  }
}
